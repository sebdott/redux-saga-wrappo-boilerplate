{
  "name": "redux-saga-wrappo-boilerplate",
  "version": "0.1.0",
  "private": true,
  "browserslist": {
    "production": [
      "> 1%",
      "last 3 Chrome versions",
      "last 3 Firefox versions",
      "last 3 Safari versions",
      "last 3 Edge versions",
      "ie >= 10"
    ],
    "development": [
      "last 2 Chrome versions",
      "last 2 Firefox versions",
      "last 2 Safari versions",
      "last 2 Edge versions",
      "IE 11"
    ]
  },
  "watch": {
    "build": {
      "patterns": [
        "src/clients/*/themes"
      ],
      "extensions": ".less"
    }
  },
  "scripts": {
    "start": "node tools/start.js",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --progress --profile --config config/webpack.config.prod.js",
    "test": "cross-env NODE_ENV=test  jest --config config/jest.config.js --bail",
    "test:watch": "cross-env NODE_ENV=test  jest --config config/jest.config.js --watch",
    "eject": "react-scripts eject",
    "lint-css": "stylelint \"src/styles/**/*.less\" --custom-formatter=node_modules/stylelint-formatter-pretty --cache",
    "lint-css:fix": "yarn lint:css --fix",
    "lint-js": "eslint -f pretty --quiet --cache \"src/**/*.{js,jsx}\"",
    "lint-js:fix": "yarn lint:js --fix",
    "precommit": "lint-staged",
    "mock": "json-server --watch ./mock/db.json -p 3001 --nc"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint -f pretty --fix",
      "prettier --write",
      "git add"
    ],
    "*.less": [
      "stylelint --custom-formatter=node_modules/stylelint-formatter-pretty --fix",
      "prettier --write",
      "git add"
    ],
    "*.{png,jpeg,jpg,svg}": [
      "imagemin-lint-staged",
      "git add"
    ]
  },
  "proxy": {
    "/api": {
      "target": "http://192.168.1.93:7500",
      "changeOrigin": true,
      "ws": true
    }
  },
  "jest": {},
  "engines": {
    "node": "^8.9.0"
  },
